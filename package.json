{
  "name": "exojs-examples",
  "description": "Playground for ExoJS examples.",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Dieter Giesler",
  "homepage": "https://exoridus.github.io/ExoJS-examples/",
  "repository": {
    "type": "git",
    "url": "git@github.com:Exoridus/ExoJS-examples.git"
  },
  "scripts": {
    "dev": "set NODE_ENV=development && rollup -cw",
    "build": "set NODE_ENV=production && rollup -c",
    "deploy": "git subtree push --prefix dist origin gh-pages",
    "prettier": "prettier src/**/*.ts --config .prettierrc --write",
    "check-ts": "tsc --noEmit --pretty",
    "check-code": "eslint ./src/**/*.ts --fix",
    "check-lit": "lit-analyzer src --strict"
  },
  "pre-commit": [
    "check-ts",
    "check-code",
    "check-lit",
    "build",
    "deploy"
  ],
  "main": "dist/examples.js",
  "browser": "dist/examples.min.js",
  "typings": "dist/examples.d.ts",
  "devDependencies": {
    "@adobe/lit-mobx": "0.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.12",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@wessberg/rollup-plugin-ts": "^1.2.28",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^9.8.4",
    "commander": "^5.1.0",
    "console-title": "^1.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "fibers": "^5.0.0",
    "lit-analyzer": "^1.2.0",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "mobx": "^5.15.4",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "rollup": "^2.15.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-prettier": "^2.1.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-sass-lint": "^1.0.0",
    "rollup-plugin-serve": "^1.0.2",
    "rollup-plugin-sizes": "^1.0.2",
    "rollup-plugin-styles": "^3.10.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "serve-static": "^1.14.1",
    "ts-lit-plugin": "^1.2.0",
    "tslib": "^2.0.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@codemirror/next": "^0.7.1",
    "dedent": "^0.7.0"
  },
  "peerDependencies": {}
}
