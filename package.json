{
  "name": "exojs-examples",
  "description": "Playground for ExoJS examples.",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Dieter Giesler",
  "homepage": "https://exoridus.github.io/ExoJS-examples/",
  "repository": {
    "type": "git",
    "url": "git@github.com:Exoridus/ExoJS-examples.git"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -cw",
    "serve": "node serve.js",
    "start": "npm-run-all -p serve watch",
    "lint": "eslint ./src",
    "ts-check": "tsc --noEmit --pretty",
    "ts-declarations": "tsc --declaration --emitDeclarationOnly --outFile dist/examples.d.ts"
  },
  "pre-commit": [
    "lint",
    "ts-check",
    "ts-declarations",
    "build"
  ],
  "main": "dist/examples.js",
  "browser": "dist/examples.min.js",
  "typings": "dist/examples.d.ts",
  "devDependencies": {
    "@adobe/lit-mobx": "0.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-html": "^0.2.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@types/dedent": "^0.7.0",
    "@types/express": "^4.17.6",
    "@types/node": "^13.13.12",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "autobind-decorator": "^2.4.0",
    "autoprefixer": "^9.8.4",
    "commander": "^5.1.0",
    "console-title": "^1.1.0",
    "eslint": "^7.4.0",
    "express": "^4.17.1",
